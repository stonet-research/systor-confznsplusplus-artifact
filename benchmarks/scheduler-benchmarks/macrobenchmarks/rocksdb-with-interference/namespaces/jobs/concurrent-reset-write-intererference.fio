[global]
name=loaded-reset
ioengine=io_uring
sqthread_poll=1
bs=64K
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1
direct=1
ioscheduler=mq-deadline

[trim]
size=700z
filename=/dev/${DEVC}
rw=randtrim
iodepth=1
numjobs=1
bs=${BS}
rate=,,${RESET_LIMIT}
exitall
exit_what=stonewall

[write]
new_group
size=100z
filename=/dev/${DEVB}
rw=write
iodepth=1
bs=64K
exitall
exit_what=stonewall