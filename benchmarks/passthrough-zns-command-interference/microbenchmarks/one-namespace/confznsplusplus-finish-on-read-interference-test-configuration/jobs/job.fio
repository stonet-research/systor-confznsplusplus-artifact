[global]
name=loaded-finish
filename=/dev/${DEV}
ioengine=io_uring_cmd 
sqthread_poll=1
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1
iodepth=1
numjobs=1
bs=64K

[fill]
rw=write
offset=${FINISH_SIZE}z
size=5z
bs=256K

[read]
stonewall
runtime=1m
ramp_time=5s
iodepth=${READ_IODEPTH}
rw=randread
bs=8K
offset=${FINISH_SIZE}z
size=5z
exitall
exit_what=all

[finish]
new_group
rw=write
size=${FINISH_SIZE}z
bs=8K
rate=,${FINISH_LIMIT},
finish=1
softfinish=0
offset=0z
exitall
exit_what=all