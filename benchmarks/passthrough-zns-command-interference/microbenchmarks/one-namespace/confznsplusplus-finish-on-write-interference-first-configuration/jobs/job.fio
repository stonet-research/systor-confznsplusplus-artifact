[global]
name=loaded-finish
filename=/dev/${DEV}
ioengine=io_uring_cmd 
direct=0
sqthread_poll=1
size=${FINISH_SIZE}z
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1
iodepth=1
numjobs=1
bs=4K

[write]
ramp_time=5s
runtime=1m
numjobs=${WRITE_IODEPTH}
rw=write
bs=8K
size=3z
offset_increment=3z
offset=11z
exitall
exit_what=all

[finish]
new_group
rw=write
bs=8K
rate=,${FINISH_LIMIT},
finish=1
softfinish=0
offset=0z
size=10z
exitall
exit_what=all