[global]
name=loaded-reset
filename=/dev/${DEV}
ioengine=io_uring_cmd 
sqthread_poll=1
size=${FILL_SIZE}z
bs=64K
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1

[fill-prep]
iodepth=1
rw=write
numjobs=1
job_max_open_zones=1

[trim]
stonewall
rw=randtrim
iodepth=1
numjobs=${TRIM_IODEPTH}
bs=${BS}
