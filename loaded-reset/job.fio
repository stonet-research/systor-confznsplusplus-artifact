[global]
name=loaded-reset
filename=/dev/${DEV}
ioengine=io_uring_cmd 
sqthread_poll=1
size=${FILL_SIZE}z
bs=64K
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1

[fill-prep]
iodepth=1
rw=write

[trim]
stonewall
rw=randtrim
iodepth=1
numjobs=${TRIM_IODEPTH}
bs=${BS}
flow_id=1
flow=${TRIM_FLOW}

[write]
rw=randwrite
iodepth=1
size=${WRITE_SIZE}z
offset=${FILL_SIZE}z
flow_id=1
flow=${WRITE_FLOW}
