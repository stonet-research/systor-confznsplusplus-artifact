[global]
name=finish
filename=/dev/${DEV}
ioengine=io_uring_cmd 
sqthread_poll=1
size=${FINISH_SIZE}z
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1
iodepth=1
numjobs=1
bs=4K
runtime=10m

[fill-read]
rw=write
offset=${FINISH_SIZE}z
size=${READ_SIZE}z

[finish]
new_group
rw=write
bs=4K
finish=1
exitall
exit_what=all

[read]
new_group
runtime=10m
ramp_time=5s
iodepth=${READ_IODEPTH}
rw=randread
bs=8K
size=${READ_SIZE}z
offset=${FINISH_SIZE}z
exitall
exit_what=all