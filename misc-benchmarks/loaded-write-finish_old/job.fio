[global]
name=loaded-finish
filename=/dev/${DEV}
ioengine=io_uring_cmd 
sqthread_poll=1
size=${FINISH_SIZE}z
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1
iodepth=1
numjobs=1
bs=4K

[write]
numjobs=${WRITE_IODEPTH}
rw=write
bs=8K
size=${CONCUR_WRITE_SIZE}z
offset_increment=${CONCUR_WRITE_SIZE}z
time_based
runtime=10
offset=${FINISH_SIZE}z
exitall

[finish]
rw=trim
bs=2147483648
rate=,,${FINISH_LIMIT}
finish=1