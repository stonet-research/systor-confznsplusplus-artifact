[global]
name=loaded-finish
filename=/dev/${DEV}
ioengine=io_uring_cmd 
sqthread_poll=1
size=${FINISH_SIZE}z
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1
iodepth=1
numjobs=1
bs=4K
runtime=10m

[append]
ramp_time=5s
numjobs=1
iodepth=${APPEND_IODEPTH}
rw=write
bs=8K
size=${APPEND_SIZE}z
offset=${FINISH_SIZE}z
exitall
exit_what=all

[finish]
new_group
rw=write
bs=4K
finish=1
exitall
exit_what=all