[global]
name=loaded-append
filename=/dev/${DEV}
ioengine=io_uring_cmd 
sqthread_poll=1
size=${APPEND_SIZE}z
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1
iodepth=1
numjobs=1
bs=4K
runtime=10m

[write]
ramp_time=5s
numjobs=${WRITE_IODEPTH}
rw=write
bs=8K
size=${CONCUR_WRITE_SIZE}z
offset_increment=${CONCUR_WRITE_SIZE}z
offset=${APPEND_SIZE}z
exitall
exit_what=all

[append]
new_group
rw=write
bs=8K
rate=,${APPEND_LIMIT},
iodepth=8
zone_append=1
exitall
exit_what=all