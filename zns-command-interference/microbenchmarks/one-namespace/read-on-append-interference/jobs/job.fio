[global]
name=loaded-append
filename=/dev/${DEV}
ioengine=io_uring_cmd 
sqthread_poll=1
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1
iodepth=1
numjobs=1
bs=4K
runtime=10m

[fill]
numjobs=1
rw=write
bs=64K
size=${WRITE_SIZE}z
offset=${APPEND_SIZE}z

[append]
stonewall
new_group
size=${APPEND_SIZE}z
iodepth=${APPEND_IODEPTH}
rw=write
bs=8K
zone_append=1
exitall
exit_what=all

[read]
new_group
offset=${APPEND_SIZE}z
numjobs=1
rate=${READ_LIMIT},,
rw=randread
iodepth=64
bs=8K
runtime=1m