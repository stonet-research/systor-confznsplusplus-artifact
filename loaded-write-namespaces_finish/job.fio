[global]
name=loaded-finish
ioengine=io_uring_cmd 
sqthread_poll=1
bs=64K
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1
runtime=10m

[finish]
new_group
filename=/dev/${DEV1}
rw=write
size=${FINISH_SIZE}z
bs=4K
rate=,${FINISH_LIMIT},
finish=1
exitall
exit_what=all

[write]
new_group
ramp_time=5s
filename=/dev/${DEV2}
rw=write
iodepth=1
size=${WRITE_SIZE}z
offset_increment=${WRITE_SIZE}z
numjobs=${WRITE_IODEPTH}
bs=8K
exitall
exit_what=all