[global]
name=loaded-reset
filename=/dev/${DEV}
ioengine=io_uring_cmd 
sqthread_poll=1
bs=64K
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1

[fill-prep]
startdelay=1m
size=${FILL_SIZE}z
iodepth=1
rw=write
numjobs=1
offset=700z
time_based
runtime=2m

[trim]
startdelay=3m
rw=randtrim
iodepth=1
numjobs=1
bs=${BS}
runtime=2m
offset=100z
size=${TRIM_SIZE}z
