[global]
name=loaded-reset
ioengine=io_uring
sqthread_poll=1
bs=64K
group_reporting
zonemode=zbd
thread=1
lat_percentiles=1
direct=1
filename=/dev/${DEV}

[trim]
size=700z
offset=${OFFSET_ZONES_C}z
rw=randtrim
iodepth=1
numjobs=1
bs=${BS}
rate=,,${RESET_LIMIT}
exitall
exit_what=stonewall

[write]
new_group
size=100z
offset=${OFFSET_ZONES_B}z
rw=write
iodepth=3
bs=8k
exitall
exit_what=stonewall